    <table class="tbl">
        <caption class="blind">보안공지</caption>
        <thead>
        <tr>
            <th style="width:100px;">번호</th>
            <% if(IS_VIEW_CATEGORY) { %>
            <th style="width:150px;">카테고리</th>
            <% } %>
            <th style="width:auto;">제목</th>
            <th style="width:150px;">날짜</th>
        </tr>
        </thead>
        <tbody id="contentsPCBody">
            <% if(result.data.length == 0 || result.data == undefined || result.data == null) { %>
                <tr>
                    <td colspan="4">등록된 게시글이 없습니다.</td>
                </tr>
            <% } %>
            <% for(var key in result.data){ %>
                <% if(key == "contains") continue; %>
                <tr>
                    <td><%=result.data[key].id%></td>
                    <% if(IS_VIEW_CATEGORY) { %>
                        <td><%=result.data[key].category%></td>
                    <% } %>
                    <td class="lt">
                        <div class="td_textarea">
                            <a href="<%=VIEW_URL%>/view/<%=result.data[key].id%>"><%=result.data[key].title%></a>
                        </div>
                    </td>
                    <td><%=moment(result.data[key].reg_timestamp).format("YYYY-MM-DD")%></td>
                </tr>
            <% } %>
        </tbody>
    </table>
    <div class="tab_mobile">
        <div class="tbl_mobile">
            <% if(result.data.length == 0 || result.data == undefined || result.data == null) { %>
            <ul>
                <li>등록된 게시글이 없습니다.</li>
            </ul>
            <% } %>
            <ul>
            <% for(var key in result.data){ %>
            <% if(key == "contains") continue; %>
                <li>
                    <p>
                        <% if(IS_VIEW_CATEGORY) { %><%=result.data[key].category%><% } %>
                        <span><%=moment(result.data[key].reg_timestamp).format("YYYY-MM-DD")%></span>
                    </p>
                    <a href="<%=VIEW_URL%>/view/<%=result.data[key].id%>"><%=result.data[key].title%></a>
                </li>
            <% } %>
            </ul>
        </div>
    </div>

    <% include ./pagination.ejs %>
